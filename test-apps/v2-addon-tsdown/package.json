{
  "name": "v2-addon-tsdown",
  "version": "0.0.0",
  "private": true,
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "start": "tsdown --watch",
    "test:fixture": "pnpm build && compare-fixture fixtures/expected-dist dist",
    "prepack": "tsdown"
  },
  "dependencies": {
    "decorator-transforms": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/preset-typescript": "^7.28.5",
    "@bosh-code/tsdown-plugin-inject-css": "^2.0.0",
    "@ember/library-tsconfig": "^1.1.3",
    "@embroider/addon-dev": "^8.1.0",
    "@nullvoxpopuli/eslint-configs": "^5.3.4",
    "@rollup/plugin-babel": "^6.1.0",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "compare-fixture": "^1.0.1",
    "concurrently": "^9.2.1",
    "ember-scoped-css": "workspace:*",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
