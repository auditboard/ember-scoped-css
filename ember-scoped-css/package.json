{
  "name": "ember-scoped-css",
  "version": "0.9.0",
  "description": "",
  "keywords": [
    "ember-addon"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./babel-plugin-scoped-class": "./src/babel-plugin-scoped-class.js",
    "./test-helper": "./src/scopedClass.js",
    "./app-css-loader": {
      "import": "./src/app-css-loader.js",
      "require": "./dist/app-css-loader.cjs"
    },
    "./app-dependency-loader": {
      "import": "./src/app-dependency-loader.js",
      "require": "./dist/app-dependency-loader.cjs"
    }
  },
  "main": "index.js",
  "scripts": {
    "esbuild": "esbuild src/scoped-babel-plugin.js src/app-css-loader.js src/app-dependency-loader.js src/lib/scoped-css-preprocessor.js --bundle --outdir=dist --platform=node --out-extension:.js=.cjs",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "prepare": "npm run esbuild",
    "release": "npx release-it",
    "test": "mocha"
  },
  "dependencies": {
    "@babel/parser": "^7.21.4",
    "blueimp-md5": "^2.19.0",
    "broccoli-concat": "^4.2.5",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.3",
    "broccoli-stew": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "ember-cli-babel": "^7.26.11",
    "ember-source": "^4.10.0",
    "ember-template-recast": "^6.1.3",
    "glob": "^8.1.0",
    "postcss": "^8.4.21",
    "postcss-selector-parser": "^6.0.11",
    "recast": "^0.22.0",
    "rollup": "^2.67.0",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "ember-template-lint": "^5.10.1",
    "esbuild": "^0.17.17",
    "eslint": "^8.42.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "release-it": "^15.10.1",
    "release-it-lerna-changelog": "^5.0.0",
    "sinon": "^15.0.1",
    "webpack": "^5.75.0"
  },
  "ember-addon": {
    "main": "src/ember-addon-main.cjs",
    "after": [
      "ember-cli-htmlbars",
      "ember-template-imports"
    ]
  },
  "peerDependencies": {
    "ember-template-lint": "^5.7.2",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ember-teplate-lint": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "../CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
