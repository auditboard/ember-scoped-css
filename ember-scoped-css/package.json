{
  "name": "ember-scoped-css",
  "version": "1.1.0",
  "description": "",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/auditboard/ember-scoped-css.git",
    "directory": "ember-scoped-css"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "files": [
    "classic-app-support",
    "src",
    "dist",
    "declarations",
    "addon-main.cjs",
    "template-registry.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./declarations/runtime/index.d.ts",
      "default": "./dist/runtime/index.js"
    },
    "./template-registry": {
      "types": "./template-registry.d.ts",
      "default": "./src/noop.js"
    },
    "./test-support": {
      "types": "./declarations/runtime/test-support.d.ts",
      "default": "./dist/runtime/test-support.js"
    },
    "./build": {
      "import": "./src/build/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./vite": {
      "import": "./src/build/vite.js"
    },
    "./helpers/scoped-class": {
      "default": "./classic-app-support/helpers/scoped-class.js"
    },
    "./addon-main.cjs": "./addon-main.cjs",
    "./babel-plugin": {
      "import": "./src/build/babel-plugin.js",
      "require": "./dist/cjs/babel-plugin.cjs"
    },
    "./template-plugin": {
      "import": "./src/build/template-plugin.js",
      "require": "./dist/cjs/template-plugin.cjs"
    }
  },
  "scripts": {
    "build": "./build.sh",
    "prepack": "./build.sh",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/parser": "^7.26.9",
    "@embroider/addon-shim": "^1.9.0",
    "babel-import-util": "^3.0.1",
    "broccoli-concat": "^4.2.5",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.3",
    "ember-template-recast": "^6.1.5",
    "glob": "^8.1.0",
    "postcss": "^8.5.3",
    "postcss-selector-parser": "^6.0.16",
    "recast": "^0.23.7",
    "unplugin": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.26.8",
    "@nullvoxpopuli/eslint-configs": "^5.3.4",
    "@tsconfig/ember": "^3.0.9",
    "@tsconfig/strictest": "^2.0.5",
    "@types/babel__core": "^7.20.5",
    "@types/common-tags": "^1.8.4",
    "@types/jscodeshift": "^17.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "common-tags": "^1.8.2",
    "concurrently": "^9.1.2",
    "content-tag": "^3.1.1",
    "ember-template-lint": "^6.1.0",
    "esbuild": "^0.25.0",
    "esbuild-plugin-vitest-cleaner": "^0.5.1",
    "eslint": "^9.36.0",
    "jscodeshift": "^17.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.2.2",
    "vitest": "^3.0.6",
    "webpack": "^5.98.0"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./helpers/scoped-class.js": "./classic-app-support/helpers/scoped-class.js"
    }
  },
  "engines": {
    "node": ">= 22.16"
  },
  "peerDependencies": {
    "ember-template-lint": ">= 5.7.2",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ember-template-lint": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "volta": {
    "extends": "../package.json"
  }
}
