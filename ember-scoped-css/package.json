{
  "name": "ember-scoped-css",
  "version": "0.7.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./babel-plugin-scoped-class": "./src/babel-plugin-scoped-class.js",
    "./test-helper": "./src/scopedClass.js",
    "./app-css-loader": {
      "import": "./src/app-css-loader.js",
      "require": "./dist/app-css-loader.cjs"
    },
    "./app-dependency-loader": {
      "import": "./src/app-dependency-loader.js",
      "require": "./dist/app-dependency-loader.cjs"
    }
  },
  "main": "index.js",
  "scripts": {
    "esbuild": "esbuild src/app-css-loader.js src/app-dependency-loader.js --bundle --outdir=dist --platform=node --out-extension:.js=.cjs",
    "lint": "eslint .",
    "prepare": "npm run esbuild",
    "release": "npx release-it",
    "test": "mocha"
  },
  "dependencies": {
    "@babel/parser": "^7.21.4",
    "blueimp-md5": "^2.19.0",
    "cheerio": "1.0.0-rc.12",
    "ember-source": "^4.10.0",
    "ember-template-recast": "^6.1.3",
    "glob": "^8.1.0",
    "postcss": "^8.4.21",
    "postcss-selector-parser": "^6.0.11",
    "recast": "^0.22.0",
    "rollup": "^2.67.0",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "ember-template-lint": "^5.7.2",
    "mocha": "^10.2.0",
    "release-it": "^15.10.1",
    "release-it-lerna-changelog": "^5.0.0",
    "sinon": "^15.0.1",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "ember-template-lint": "^5.7.2",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ember-teplate-lint": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "../CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
