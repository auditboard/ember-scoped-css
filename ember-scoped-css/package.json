{
  "name": "ember-scoped-css",
  "version": "0.11.0",
  "description": "",
  "keywords": [
    "ember-addon"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "files": [
    "src",
    "dist",
    "addon-main.cjs"
  ],
  "exports": {
    "./test-support": "./src/runtime/test-support.js",
    "./build": "./src/build/index.js",
    "./build/app-css-loader": {
      "import": "./src/build/app-css-loader.js",
      "require": "./dist/build/app-css-loader.cjs"
    },
    "./build/app-dependency-loader": {
      "import": "./src/build/app-dependency-loader.js",
      "require": "./dist/build/app-dependency-loader.cjs"
    },
    "./build/ember-classic-support": {
      "require": "./dist/lib/scoped-css-preprocessor.cjs"
    },
    "./addon-main.cjs": "./addon-main.cjs",
    "./babel-plugin": {
      "import": "./src/scoped-babel-plugin.js",
      "require": "./dist/scoped-babel-plugin.cjs"
    }
  },
  "scripts": {
    "build": "./build.sh",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "test": "mocha"
  },
  "dependencies": {
    "@babel/parser": "^7.21.4",
    "@embroider/addon-shim": "^1.0.0",
    "blueimp-md5": "^2.19.0",
    "broccoli-concat": "^4.2.5",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.3",
    "broccoli-stew": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "ember-cli-babel": "^7.26.11",
    "ember-source": "^4.10.0",
    "ember-template-recast": "^6.1.3",
    "find-up": "^6.3.0",
    "glob": "^8.1.0",
    "postcss": "^8.4.21",
    "postcss-selector-parser": "^6.0.11",
    "recast": "^0.22.0",
    "rollup": "^2.67.0",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "ember-template-lint": "^5.7.2",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "sinon": "^15.0.1",
    "webpack": "^5.75.0"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "engines": {
    "node": ">= 18"
  },
  "peerDependencies": {
    "ember-template-lint": "^5.7.2",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ember-teplate-lint": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
