{
  "name": "ember-scoped-css",
  "version": "0.18.0",
  "description": "",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "files": [
    "src",
    "dist",
    "declarations",
    "addon-main.cjs",
    "template-registry.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./declarations/runtime/index.d.ts",
      "default": "./dist/runtime/index.js"
    },
    "./template-registry": {
      "types": "./template-registry.d.ts",
      "default": "./src/noop.js"
    },
    "./test-support": {
      "types": "./declarations/runtime/test-support.d.ts",
      "default": "./dist/runtime/test-support.js"
    },
    "./build": {
      "import": "./src/build/index.js",
      "require": "./dist/cjs/build/index.cjs"
    },
    "./build/app-css-loader": {
      "import": "./src/build/app-css-loader.js",
      "require": "./dist/cjs/build/app-css-loader.cjs"
    },
    "./build/app-dependency-loader": {
      "import": "./src/build/app-dependency-loader.js",
      "require": "./dist/cjs/build/app-dependency-loader.cjs"
    },
    "./build/ember-classic-support": {
      "require": "./dist/cjs/build/ember-classic-support.cjs"
    },
    "./addon-main.cjs": "./addon-main.cjs",
    "./babel-plugin": {
      "import": "./src/build/babel-plugin.js",
      "require": "./dist/cjs/scoped-babel-plugin.cjs"
    }
  },
  "scripts": {
    "build": "./build.sh",
    "prepack": "./build.sh",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/parser": "^7.23.5",
    "@embroider/addon-shim": "^1.8.6",
    "acorn": "^8.11.2",
    "blueimp-md5": "^2.19.0",
    "broccoli-concat": "^4.2.5",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.3",
    "broccoli-stew": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "content-tag": "^2.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-source": "^4.10.0",
    "ember-template-recast": "^6.1.3",
    "find-up": "^5.0.0",
    "glob": "^8.1.0",
    "postcss": "^8.4.21",
    "postcss-selector-parser": "^6.0.11",
    "recast": "^0.23.4",
    "rollup": "^2.67.0",
    "unplugin": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@glimmer/component": "^1.1.2",
    "@nullvoxpopuli/eslint-configs": "^4.0.0",
    "@tsconfig/ember": "^3.0.1",
    "@tsconfig/strictest": "^2.0.2",
    "@types/blueimp-md5": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "concurrently": "^8.2.2",
    "ember-template-lint": "^6.0.0",
    "esbuild": "^0.19.0",
    "esbuild-plugin-vitest-cleaner": "^0.5.1",
    "eslint": "^8.50.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0",
    "webpack": "^5.75.0"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "engines": {
    "node": ">= 18"
  },
  "peerDependencies": {
    "ember-template-lint": "^5.7.2",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ember-teplate-lint": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
