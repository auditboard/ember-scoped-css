{
  "name": "stylelint-ember-scoped-css",
  "version": "1.0.0",
  "description": "Stylelint plugin and config for ember-scoped-css",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "stylelint-config",
    "ember",
    "ember-scoped-css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/auditboard/ember-scoped-css.git",
    "directory": "stylelint-ember-scoped-css"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./config": "./src/config.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "echo \"Build skipped\"",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "test": "vitest"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^5.3.4",
    "@tsconfig/strictest": "^2.0.5",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "postcss": "^8.5.6",
    "postcss-selector-parser": "^6.1.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "typescript": "^5.9.2",
    "vitest": "^3.0.6",
    "vitest-stylelint-utils": "^2.0.0"
  },
  "peerDependencies": {
    "stylelint": "^16.0.0"
  },
  "engines": {
    "node": ">= 22.16"
  },
  "volta": {
    "extends": "../package.json"
  }
}
